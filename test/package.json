{
  "name": "bedrock-passport-test",
  "version": "0.0.1-0",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test ESM_OPTIONS='{cache:false}' nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test ESM_OPTIONS='{cache:false}' nyc --reporter=lcovonly npm test",
    "coverage-report": "nyc report"
  },
  "dependencies": {
    "bedrock": "^4.5.0",
    "bedrock-account": "^6.3.2",
    "bedrock-express": "^6.4.1",
    "bedrock-mongodb": "^8.5.0",
    "bedrock-passport": "file:..",
    "bedrock-server": "^3.2.1",
    "bedrock-test": "^6.0.0",
    "bedrock-validation": "^5.6.3",
    "cross-env": "^7.0.2",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock-passport/**"
    ],
    "exclude": [
      "node_modules/bedrock-passport/node_modules/**"
    ]
  }
}
